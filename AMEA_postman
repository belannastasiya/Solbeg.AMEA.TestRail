		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"postman.setEnvironmentVariable(\"utils\", () => {",
					"    var getRun_id = (projectID) => {     ",
					"",
					"        var Project_ID = pm.environment.get(\"projectID\");",
					"        Project_ID = String (Project_ID) ",
					"",
					"    const echoGetRequest = {",
					"        url: '{{TR_url}}/index.php?/api/v2/get_runs/project_ID',",
					"        \"auth\": {",
					"            \"type\": \"basic\",",
					"            \"basic\": [",
					"                {",
					"                    \"key\": \"password\",",
					"                    \"value\": \"{{TR_pass}}\",",
					"                    \"type\": \"string\"",
					"                    },",
					"                {",
					"                    \"key\": \"username\",",
					"                    \"value\": \"{{TR_user}}\",",
					"                    \"type\": \"string\"",
					"                    }",
					"                    ]",
					"                },",
					"        method: \"GET\",",
					"        header: {",
					"            key: \"Content-Type\",",
					"            name: \"Content-Type\",",
					"            value: \"application/json\",",
					"            type: \"text\",",
					"            },",
					"        body: {",
					"            }",
					"            };",
					"    pm.sendRequest(echoGetRequest, function (err, res) {",
					"    let a = res.text();",
					"    let jsondata = JSON.parse(a);",
					"    var jsondata = pm.response.json().runs //Забираем поле runs/",
					"    var old_TR_name = pm.environment.get(\"old_TR_name\");",
					"",
					"    // перебираем элементы runs",
					"    for (var i = 0; i<jsondata.length; i++) {",
					"    //сравниваем элементы с искомым",
					"        if (jsondata[i].name.includes(old_TR_name)) {",
					"    //если нашли совпадение, записываем в переменную",
					"        pm.collectionVariables.set(\"run_id\", jsondata.runs[i].id);",
					"        };",
					"    };",
					"    pm.test(\"Can see Runs list\", function () {",
					"        pm.response.to.have.status(200);",
					"        });",
					"    });",
					"    }",
					"",
					"    var getCase_id = (projectID) => {     ",
					"        ",
					"        var Project_ID = pm.environment.get(\"projectID\");",
					"        Project_ID = String (Project_ID)  ",
					"    ",
					"    const echoGetRequest = {",
					"        url: '{{TR_url}}/get_cases/Project_ID&type_id={{RegressionID}}&offset={{number}}', // переменная number показывает сколько элементов мы пропускаем от начала списка кейсов/",
					"        ",
					"        \"auth\": {",
					"            \"type\": \"basic\",",
					"            \"basic\": [",
					"                {",
					"                    \"key\": \"password\",",
					"                    \"value\": \"{{TR_pass}}\",",
					"                    \"type\": \"string\"",
					"                    },",
					"                {",
					"                    \"key\": \"username\",",
					"                    \"value\": \"{{TR_user}}\",",
					"                    \"type\": \"string\"",
					"                    }",
					"                    ]",
					"                },",
					"        method: \"GET\",",
					"        header: {",
					"            key: \"Content-Type\",",
					"            name: \"Content-Type\",",
					"            value: \"application/json\",",
					"            type: \"text\",",
					"            },",
					"        body: {",
					"            }",
					"            };",
					"",
					"    var idCases = []; //создаём массив",
					"",
					"    pm.collectionVariables.set(\"number\", 0); //переменная которая будет указывать кол-во id которое надо пропустить при повторе запроса (+ нам каждый раз когда запрос используется надо начинать с 0)",
					"",
					"    var next = null;",
					"",
					"    do {",
					"",
					"    pm.sendRequest(echoGetRequest, function (err, res) {",
					"",
					"    let a = res.text();",
					"    let jsondata = JSON.parse(a);",
					"    next = jsondata._links.next;",
					"    jsondata = jsondata.cases; // приводим тело ответа в божеский вид",
					"",
					"    var idCases0 = [];",
					"",
					"    for (var i = 0; i<jsondata.length; i++) {",
					"    idCases0.push(jsondata[i].id); //Добавляем id каждого в массив",
					"    };",
					"",
					"    pm.collectionVariables.set = (\"number\", number + 250); // увеличиваем переменную для следующего цикла",
					"",
					"    idCases.push(idCases0) // перекладываем текущее значение в результирующую переменную",
					"    });",
					"",
					"    } while (next != null)",
					"",
					"    pm.collectionVariables.set(\"case_ids\", idCases);",
					"    }",
					"",
					"    return {",
					"            myPackage: {",
					"                getRun_id,",
					"                getCase_id",
					"            }",
					"        };",
					"    });"
				]
			}
		},
